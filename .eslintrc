{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "no-console": "off",
    "indent": [
      "error",
      2
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "never"
    ],
    "camelcase": 1,
    "complexity": [
      2,
      5
    ],
    "comma-spacing": 1,
    "consistent-return": 0,
    "eol-last": 1,
    "eqeqeq": [
      2,
      "smart"
    ],
    "max-depth": [
      1,
      2
    ],
    "max-len": [
      2,
      110
    ],
    "max-params": [
      1,
      4
    ],
    "max-statements": [
      1,
      15
    ],
    "no-use-before-define": [
      2,
      "nofunc"
    ],
    "object-curly-spacing": [
      "error",
      "always"
    ],
    "no-multi-spaces": 1,
    "no-process-exit": 0,
    "no-trailing-spaces": 1,
    "no-underscore-dangle": 0,
    "no-unused-vars": 1,
    "space-infix-ops": 1,
    "semi-spacing": 1,
    "yoda": [
      1,
      "never"
    ]
  }
}
